
@{
    ViewData["Title"] = "Create";
}


<h1 class="text-center">Add data</h1>

<form method="post">
    <div class="form-group ">
    <label >ENTER NAME</label>
    <input name="names" type="text" class="form-control" placeholder="Input name">
  </div>
 
  <div class="form-group ">
    <label >Enter DisplayOrder</label>
    <input name="displays"  type="text" class="form-control" placeholder="Display">
  </div>

  <button type="submit" class="btn btn-success">Submit</button>
  <button  asp-controller="Category" asp-action="Index" class="btn btn-success">Back</button>
</form>


